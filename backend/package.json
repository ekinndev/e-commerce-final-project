{
    "name": "backend-starter-template",
    "version": "1.0.0",
    "description": "This is a dockerized express server template",
    "main": "index.js",
    "scripts": {
        "test": "jest --verbose ./src",
        "test:coverage": "jest --verbose --coverage ./src",
        "start": "NODE_ENV=development node ./bin/www",
        "start:prod": "NODE_ENV=production node ./bin/www",
        "build": "tsc --outDir ./build ",
        "watch": "tsc-watch --outDir ./build  --onSuccess \"node ./bin/www\""
    },
    "author": "Ekin Abalioglu",
    "license": "MIT",
    "dependencies": {
        "@google-cloud/storage": "^6.9.5",
        "async-validator": "^4.0.7",
        "axios": "^1.3.5",
        "connect-mongo": "^5.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "debug": "^4.3.2",
        "dotenv": "^16.0.3",
        "express": "^5.0.0-beta.1",
        "express-session": "^1.17.3",
        "mongoose": "^7.0.3",
        "mongoose-autopopulate": "^1.0.0",
        "multer": "^1.4.5-lts.1",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^8.0.0",
        "sharp": "^0.32.0",
        "supertest": "^6.2.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.7",
        "@types/jest": "^29.4.0",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.13.0",
        "@types/passport": "^1.0.12",
        "@types/sharp": "^0.31.1",
        "@types/uuid": "^9.0.1",
        "jest": "^29.4.2",
        "prettier": "^2.5.1",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^4.9.5"
    }
}
